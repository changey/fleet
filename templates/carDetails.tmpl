<div>
  <table>
    <tr>
      <td>
        Make
      </td>
      <td>
        <%= make %>
      </td>
    </tr>
    <tr>
      <td>
        Model
      </td>
      <td>
        <%= model %>
      </td>
    </tr>
    <tr>
      <td>
        Car ID
      </td>
      <td>
        <%= _id %>
      </td>
    </tr>
  </table>
  <br></br>
  <% if(state === "U") { %>
  <button class="change-state" data-action="reserve" data-id="<%= _id %>">Reserve</button>
  <% } %>
  <% if(state === "UP") { %>
  <button class="change-state" data-id="<%= _id %>">Occupy</button>
  <% } %>
  <% if(state === "O") { %>
  <button class="change-state" data-id="<%= _id %>">Schedule a return</button>
  <% } %>
  <% if(state === "OP") { %>
  <button class="change-state" data-id="<%= _id %>">Vacate</button>
  <% } %>
</div>